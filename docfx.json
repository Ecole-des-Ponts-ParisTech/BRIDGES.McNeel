{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**/BRIDGES.McNeel.RhinoCommon.csproj",
            "src/**/BRIDGES.McNeel.Grasshopper.csproj",
          ],
	  "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/darkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appFaviconPath": "images/logo_enpc.svg",
      "_appTitle": "BRIDGES.McNeel Documentation",
      "_appLogoPath": "images/logo_enpc.svg",
      "_appName": "BRIDGES.McNeel",
      "_enableSearch": true,
      "_disableAffix": false,
      "_enableNewTab": true,
      "_appFooter": "By ENPC <br/>Generated by <b>DocFX</b>",
      "_disableSideFilter": false,
      "_gitContribute": {
        "repo": "https://github.com/Ecole-des-Ponts-ParisTech/BRIDGES.McNeel"
      }
    }
  }
}
